<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<BorderPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <VBox alignment="CENTER" spacing="50.0" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER" spacing="50.0">
               <children>
                  <TextField id="sellerIdTextField" promptText="Seller Id" />
                  <Button id="removeSellerBtn" mnemonicParsing="false" style="-fx-background-color: coral;" text="Remove Seller" />
               </children>
            </HBox>
            <HBox alignment="CENTER" spacing="50.0">
               <children>
                  <TextField id="customerIdTextField" promptText="Customer Id" />
                  <Button id="removeCustomerBtn" mnemonicParsing="false" style="-fx-background-color: coral;" text="Remove Customer" />
               </children>
            </HBox>
         </children>
      </VBox>
   </center>
   <top>
      <HBox alignment="TOP_RIGHT" BorderPane.alignment="CENTER">
         <children>
            <Button id="logoutBtn" mnemonicParsing="false" style="-fx-background-color: orange;" text="Logout" />
         </children>
         <BorderPane.margin>
            <Insets right="10.0" top="10.0" />
         </BorderPane.margin>
      </HBox>
   </top>
</BorderPane>
